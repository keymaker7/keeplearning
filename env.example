# 데이터베이스 연결 문자열
# Neon Database 또는 다른 PostgreSQL 호스팅 서비스 사용
DATABASE_URL=postgresql://user:password@host:5432/database

# OpenAI API 키
# https://platform.openai.com/api-keys 에서 발급받으세요
OPENAI_API_KEY=sk-your-openai-api-key-here

# 서버 포트 (기본값: 5000)
PORT=5000

# 세션 암호화 키 (랜덤 문자열 사용 권장)
# 예: openssl rand -base64 32 명령어로 생성 가능
SESSION_SECRET=your-random-session-secret-key-here

